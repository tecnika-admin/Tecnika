<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence -->
        <record id="sequence_tablas_cfdi_reference" model="ir.sequence">
            <field name="name">Tablas CFDI Reference</field>
            <field name="code">tablas.cfdi.reference</field>
            <field name="prefix">TABLA/</field>
            <field name="padding">3</field>
        </record>

        <!-- Sequences for incapacidades.nomina -->
        <record id="seq_horas_nomina" model="ir.sequence">
            <field name="name">Horas Extras nomina</field>
            <field name="code">horas.nomina</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for incapacidades.nomina -->
        <record id="seq_nomina" model="ir.sequence">
            <field name="name">Numero nomina</field>
            <field name="code">numero.nomina</field>
            <field name="prefix">NOM/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for caja ahorro -->
        <record id="seq_caja_ahorro" model="ir.sequence">
            <field name="name">Caja Ahorro</field>
            <field name="code">caja.nomina</field>
            <field name="prefix">CA/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for incidencias.nomina -->
        <record id="seq_incidencias_nomina" model="ir.sequence">
            <field name="name">Incidencias nomina</field>
            <field name="code">incidencias.nomina</field>
            <!-- <field name="prefix">SO</field> -->
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>

        <!-- Sequences for regardo.nomina -->
        <record id="seq_retardo_nomina" model="ir.sequence">
            <field name="name">Retardo nomina</field>
            <field name="code">retardo.nomina</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>

        <!-- Sequences for credito.infonavit -->
        <record id="seq_credito_infonavit" model="ir.sequence">
            <field name="name">Credito infonvit</field>
            <field name="code">credito.infonavit</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>

        <!-- Sequences for prima dominical-->
        <record id="seq_prima_dominical" model="ir.sequence">
            <field name="name">Prima dominical</field>
            <field name="code">prima.dominical</field>
            <field name="padding">4</field>
            <field name="company_id" eval="True"/>
        </record>

    </data>
</odoo>
